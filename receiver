#include<SoftwareSerial.h>

SoftwareSerial mybt(0,1);

String m;
void setup()
{
  Serial.begin(38400);
  mybt.begin(38400);
  pinMode(5,OUTPUT);
  pinMode(6,OUTPUT);
  pinMode(7,OUTPUT);
  pinMode(8,OUTPUT);
  
}

void loop()
  
{
   if (mybt.available())
  {
  
   m = mybt.readString();
   Serial.print(m);
  
  }
   if (m =="f")
   {
     digitalWrite(5,0);
     digitalWrite(6,1);
     digitalWrite(7,1);
     digitalWrite(8,0);
     delay(200);
   }
